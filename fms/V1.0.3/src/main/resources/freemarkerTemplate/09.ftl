 <Worksheet ss:Name="客户基本信息表（富涌）">
  <Table ss:ExpandedColumnCount="12" ss:ExpandedRowCount="250" x:FullColumns="1"
   x:FullRows="1" ss:StyleID="s16" ss:DefaultColumnWidth="53.25"
   ss:DefaultRowHeight="25.3125">
   <Column ss:StyleID="s16" ss:AutoFitWidth="0" ss:Width="78.75"/>
   <Column ss:StyleID="s16" ss:AutoFitWidth="0" ss:Width="153"/>
   <Column ss:StyleID="s16" ss:AutoFitWidth="0" ss:Width="295.5"/>
   <Column ss:StyleID="s16" ss:AutoFitWidth="0" ss:Width="28.5" ss:Span="6"/>
   <Column ss:Index="12" ss:StyleID="s16" ss:Width="123"/>
   <Row ss:AutoFitHeight="0"/>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeAcross="2" ss:StyleID="s29"><Data ss:Type="String">客户基本信息表</Data></Cell>
    <Cell ss:StyleID="s24"/>
    <Cell ss:StyleID="s24"/>
    <Cell ss:StyleID="s24"/>
    <Cell ss:StyleID="s24"/>
    <Cell ss:StyleID="s24"/>
    <Cell ss:StyleID="s24"/>
    <Cell ss:StyleID="s24"/>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:StyleID="s23"><Data ss:Type="String">名称</Data></Cell>
    <Cell ss:MergeAcross="1" ss:StyleID="s30"><Data ss:Type="String">明细</Data></Cell>
   </Row>
   
   <#list loanPawnList as loanPawn>
   
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeDown="1" ss:StyleID="m90989864"><Data ss:Type="String">产权人信息</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">产权人</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String"><#assign ftlNum = 0 /><#list loanPawn.propertyOwnerList as people><#if ftlNum &gt; 0>，</#if>${people.name?if_exists}<#assign ftlNum = ftlNum + 1 /></#list></Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">身份证号码</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String"><#assign ftlNum = 0 /><#list loanPawn.propertyOwnerList as people><#if ftlNum &gt; 0>，</#if>${people.certificateNumber?if_exists}<#assign ftlNum = ftlNum + 1 /></#list></Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeDown="8" ss:StyleID="m90989884"><Data ss:Type="String">抵押房产信息</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">抵押房屋地址</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">${loanPawn.propertyAddress?if_exists}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">房产证号码</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">${loanPawn.certificateNumber?if_exists}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">建筑面积</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">${loanPawn.coveredArea?if_exists}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">房屋性质</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">${loanPawn.propertyType?if_exists}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">房型</Data></Cell>
    <Cell ss:StyleID="s17"/>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">总楼层</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">${loanPawn.totalFloors?if_exists}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">户口人数</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">${loanPawn.householdInfoList?size}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">竣工年份</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">${loanPawn.completionDate?if_exists}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">房屋评估价值</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">${loanPawn.valuation?if_exists}元</Data></Cell>
   </Row>
  
   <#assign privateLoan = 0 />
   <#assign bankLoan = 0 />
   <#list loanPawn.propertyAdjustmentInfoList as propertyAdjustmentInfo>
		<#if propertyAdjustmentInfo.mortgageProperty ?? && propertyAdjustmentInfo.mortgageProperty == "银行借贷">
			<#assign bankLoan = bankLoan + propertyAdjustmentInfo.mortgagePrice />
		</#if>
		<#if propertyAdjustmentInfo.mortgageProperty ?? && propertyAdjustmentInfo.mortgageProperty == "民间借贷">
			<#assign privateLoan = privateLoan + propertyAdjustmentInfo.mortgagePrice />
		</#if>
	</#list>
   <Row ss:AutoFitHeight="0">
    <Cell ss:MergeDown="2" ss:StyleID="m90989824"><Data ss:Type="String">房贷贷款情况</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">房屋银行抵押余额（若有）</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String"><#if bankLoan &gt; 0>${bankLoan?number}万元</#if></Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">房屋私人抵押余额（若有）</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String"><#if privateLoan &gt; 0>${privateLoan?number}万元</#if></Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:Index="2" ss:StyleID="s17"><Data ss:Type="String">所借款项是否归还以上抵押</Data></Cell>
    <Cell ss:StyleID="s17"/>
   </Row>
   
   </#list>
   
   <Row ss:AutoFitHeight="0">
    <Cell ss:StyleID="s18"><Data ss:Type="String">贷款金额</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">拟融资金额</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">${loanAmount?if_exists}元</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:StyleID="s18"><Data ss:Type="String">备用房</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String">备用房地址（若有）</Data></Cell>
    <Cell ss:StyleID="s17"><Data ss:Type="String"><#if loanSepraroomList ??><#assign ftlNum = 0 /><#list loanSepraroomList as loanSepraroom><#if ftlNum &gt; 0>，</#if>${loanSepraroom.propertyAddress?if_exists}<#assign ftlNum = ftlNum + 1 /></#list></#if></Data></Cell>
   </Row>
  </Table>
